{
	"swagger": "2.0",
	"info": {
		"version": "0.0.1",
		"title": "VentUMN Node.js Mobile Application API",
		"description": "VentUMN Node.js Mobile Application API"
	},
	"host": "https://ventumn-backend.herokuapp.com",
	"basePath": "/",
	"schemes": [
		"https"
	],
	"produces": [
		"application/json",
		"text/plain"
	],
	"paths": {
		"/docs": {
			"get": {
				"description": "Swagger Documentations Page"
			}
		},
		"/signUp": {
			"post": {
				"description": "User Registration",
				"responses": {
					"200": {
						"description": "Success",
						"examples": {
							"text/plain": "You are Registered!"
						}
					}
				}
			},
			"produces": [
				"text/plain"
			],
			"parameters": [
				{
					"in": "body",
					"required": true,
					"schema": {
						"example": {
							"email": "test@email.com",
							"name": "John Doe",
							"passwd1": "password"
						}
					}
				}
			]
		},
		"/checkLogin": {
			"post": {
				"description": "User Login",
				"responses": {
					"200": {
						"description": "Success",
						"examples": {
							"text/plain": "<user_id>"
						}
					},
					"401": {
						"description": "Not found",
						"examples": {
							"text/plain": "Sorry, no one's"
						}
					}
				}
			},
			"produces": [
				"text/plain"
			],
			"parameters": [
				{
					"in": "body",
					"required": true,
					"schema": {
						"example": {
							"email": "test@email.com",
							"passwd1": "password"
						}
					}
				}
			]
		},
		"/getData": {
			"post": {
				"description": "Get All Event",
				"responses": {
					"200": {
						"description": "Success",
						"examples": {
							"application/json": [
								{
									"prodi": "prodi",
									"nama": "nama",
									"price": "10000",
									"deskripsi": "deskripsi",
									"tanggalMulai": "2018-04-21",
									"tanggalSelesai": "2018-04-24",
									"waktuMulai": "10:00",
									"waktuSelesai": "12:00",
									"poster": "poster",
									"latLng": "-6.0451,60.123",
									"lokasi": "lokasi"
								}
							]
						}
					}
				}
			},
			"produces": [
				"application/json"
			]
		},
		"/getOneData/{id}": {
			"post": {
				"description": "Get One Event",
				"responses": {
					"200": {
						"description": "Success",
						"examples": {
							"application/json": {
								"prodi": "prodi",
								"nama": "nama",
								"price": "10000",
								"deskripsi": "deskripsi",
								"tanggalMulai": "2018-04-21",
								"tanggalSelesai": "2018-04-24",
								"waktuMulai": "10:00",
								"waktuSelesai": "12:00",
								"poster": "poster",
								"latLng": "-6.0451,60.123",
								"lokasi": "lokasi"
							}
						}
					}
				}
			},
			"produces": [
				"application/json"
			],
			"parameters": [
				{
					"name": "id",
					"in": "path",
					"required": true,
					"schema": {
						"type": "string"
					}
				}
			]
		}
	},
	"consumes": [
		"application/json"
	]
}