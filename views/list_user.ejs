<br>
    <!-- Main content -->
    <section class="content">
      <div class="row">
        <div class="col-xs-12">
          <div class="box">
            <div class="box-header">
                  <h3 class="box-title">Data Table With Full Features</h3>
                  <div class="box-title" style="padding-left: 685px">
                  <a href="/daftar">
                  <button href="/daftar" >Create New User</button>
                  </a>
                  </div>
                  
            </div>
            <!-- /.box-header -->
            <div class="box-body">
              <table id="example1" class="table table-bordered table-striped">
                <thead>
                <tr>
                  <th style="display:none">ID </th>
                  <th>Email</th>
                  <th>Nama Lengkap</th>
                  <th>Action</th>
                </tr>
                </thead>
                <tbody>
                  <tr class="admin">
                  <% for(var i=0; i<admin.length; i++) { %>
                  <td style="display: none"><%= admin[i]._id %></td>
                  <td><%= admin[i].email %></td>
                  <td><%= admin[i].name %></td>
                  <td>
                    <div class="col-md-2">
                  <form action="/delete/<%=admin[i]._id%>" method="POST">
                    <button type="submit">Delete</button>
                  </form>
                </div>
                <div class="col-md-offset-5 ">
                  <form action="/updateuser/<%=admin[i]._id%>" method="POST">
                  <button type="submit">Update</button>
                </form>
                  </div>
                  </td>
                  </tr>
                  <% } %>
                </tbody>
                <tfoot>
                <tr>
                  <th>Email</th>
                  <th>Nama Lengkap</th>
                  <th>Action</th>
                </tr>
                </tfoot>
              </table>
            </div>
            <!-- /.box-body -->
          </div>
          <!-- /.box -->
        </div>
        <!-- /.col -->
      </div>
      <!-- /.row -->
    </section>
    <!-- /.content -->

</div>